{
    "name": "jupyterdotnetnotebooks",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "jupyterlabwithdotnet",  // The name of the service in the docker-compose file
    "workspaceFolder": "/home/user/notebooks",  // The path inside the container where your workspace will be mounted
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ]
        },
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            }
        }
}